{
	"name": "Octipedia",
	"author": "...",
	"url": "https://octaria.wiki.gd/",
	"descriptionmsg": "A skin for Octipedia",
	"namemsg": "skinname-octipedia",
	"license-name": "GPL-2.0-or-later",
	"type": "skin",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"ValidSkinNames": {
		"nostalgia": {
			"class": "SkinTemplate",
			"args": [
				{
					"name": "nostalgia",
					"template": "MediaWiki\\Skin\\Octipedia\\OctipediaTemplate",
					"styles": [
						"ext.octipedia"
					],
					"bodyOnly": true
				}
			]
		}
	},
	"MessagesDirs": {
		"Octipedia": [
			"i18n"
		]
	},
	"ResourceModules": {
		"ext.octipedia": {
			"class": "ResourceLoaderSkinModule",
			"styles": {
				"0": "style.css"
			}
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteSkinPath": "Octipedia/modules"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Skin\\Octipedia\\": "includes/"
	},
	"manifest_version": 2
}
